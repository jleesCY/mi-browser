{
  "expo": {
    "name": "mi",
    "slug": "mi-browser",
    "version": "0.3.2",
    "orientation": "portrait",
    "scheme": "mibrowser",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "android": {
        "package": "com.jlees.mibrowser",
        "permissions": [
            "CAMERA",
            "RECORD_AUDIO",
            "ACCESS_FINE_LOCATION",
            "ACCESS_COARSE_LOCATION",
            "MODIFY_AUDIO_SETTINGS"
        ],
        "intentFilters": [
            {
            "action": "VIEW",
            "autoVerify": true,
            "data": [
                { "scheme": "http" },
                { "scheme": "https" }
            ],
            "category": [
                "BROWSABLE",
                "DEFAULT"
            ]
            }
        ]
    },
    "extra": {
      "eas": {
        "projectId": "9f3df663-74f1-42b5-beee-1fa981dee3ff"
      }
    }
  }
}
